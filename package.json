{
  "name": "boleto-br",
  "version": "0.0.1",
  "description": "A tool to generate brazilian banks boleto",
  "main": "dist/index.js",
  "scripts": {
    "prettier": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma none --write \"lib/**/*.js\" --write \"test/**/*.js\" --write rollup.config.js",
    "flow": "flow",
    "pretest": "npm run flow && npm run prettier && xo --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "npm test",
    "build": "rollup -c"
  },
  "keywords": [
    "boleto"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-0",
      "flow"
    ],
    "plugins": [
      [
        "flow-runtime",
        {
          "annotate": true,
          "assert": true
        }
      ]
    ]
  },
  "author": "Rafael Castro",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "flow-bin": "^0.49.1",
    "flow-runtime": "^0.13.0",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "prettier": "^1.4.4",
    "rollup": "^0.45.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-filesize": "^1.4.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "envs": [
      "node",
      "jest"
    ],
    "rules": {
      "no-mixed-operators": 0
    },
    "parser": "babel-eslint"
  },
  "dependencies": {
    "pad-end": "^1.0.2",
    "pad-start": "^1.0.2",
    "vanilla-commons": "^1.0.1"
  }
}
